    "# 定义评估\n",
    "def evaluation(model, params_file_path):\n",
    "    \n",
    "    # 加载模型训练参数\n",
    "    model_state_dict = paddle.load(params_file_path)\n",
    "    model.load_dict(model_state_dict)\n",
    "    \n",
    "    # 开启预测模式， 准备验证集数据\n",
    "    model.eval()\n",
    "    eval_loader = data_loader(DATADIR , batch_size=10 , mode = 'eval')\n",
    "    \n",
    "    acc_set = []\n",
    "    avg_loss_set = []\n",
    "    \n",
    "    for batch_id , data in enumerate(eval_loader()):\n",
    "        imgs , labels = data\n",
    "        imgs = paddle.to_tensor(imgs)\n",
    "        labels = labels.astype(np.int64)\n",
    "        labels = paddle.to_tensor(labels)\n",
    "        \n",
    "        predictions , acc  = model(imgs,labels)\n",
    "        loss = F.binary_cross_entropy_with_logits(predictions,labels)\n",
    "        avg_loss = paddle.mean(loss)\n",
    "        avg_loss_set.append(float(avg_loss.numpy()))\n",
    "        acc_set.append(float(acc.numpy()))\n",
    "    acc_val_mean = np.array(acc_set).mean()\n",
    "    avg_loss_val_mean = np.array(avg_loss_set).mean()\n",
    "    \n",
    "    print(f'acc平均:{acc_val_mean} ; loss平均:{avg_loss_val_mean}')\n",
    "    \n",
    "    \n",
    "    \n",
    "    \n",
    "evaluation(model, params_file_path=\"palm.pdparams\") "
   ]
  },
